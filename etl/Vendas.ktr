  <step>
    <name>InputFatoVenda</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>ERPDEMO</connection>
    <sql>select   
  coalesce(i.ift_item, 'N/A') as ift_item,
   i.ift_quantfatc,
   i.ift_valitm as ift_valor, 
   (i.ift_valitm - (i.ift_custitm * i.ift_quantfatc)) as lucro,
   f.fat_loja,
   f.fat_codven,
   f.fat_codpes,
  cast(to_char(f.fat_datfat, 'yyyyMMDD') as integer) as fat_data,
  coalesce(p.pes_codpes,'N/A') as pes_codpes , 
  coalesce(p.pes_nome,'N/A') as pes_nome , 
  coalesce(p.pes_endere,'N/A') as pes_endere, 
  coalesce(p.pes_bairro,'N/A') as pes_bairro , 
  coalesce(p.pes_cidade,'N/A') as pes_cidade, 
  coalesce(p.pes_uf,'N/A') as pes_uf, 
  coalesce(p.pes_cep,'N/A')as pes_cep, 
  coalesce(p2.pes_codpes,'N/A') as codpesV,
  coalesce(p2.pes_nome,'N/A') as pes_nomeV,
  coalesce(p2.pes_endere,'N/A') as pes_endereV,
  coalesce(p2.pes_bairro,'N/A') as pes_bairroV,
  coalesce(p2.pes_cidade,'N/A') as pes_cidadeV,
  coalesce(p2.pes_uf,'N/A') as pes_ufV,
  coalesce(p2.pes_cep,'N/A') as pes_cepV,
   e.emp_codemp,
   e.emp_razsoc,
   l.loj_codloj,
   l.Loj_denloj,
   l.loj_endloj,
   l.loj_bailoj,
   l.loj_ufloj,
   l.loj_cidloj,
   l.loj_ceploj,
   ip.itm_item,
   coalesce(g.grp_dscgrp,'N/A') as grp_dscgrp,
   ip.itm_descri,
   ip.itm_unidade,
   
  case when ip.itm_marca = '' or ip.itm_marca is null then 'N/A' else ip.itm_marca end as itm_marca
from
   fatura f 
   JOIN
      item_fatura i 
      on i.ift_numfat = f.fat_numfat 
      and i.ift_loja = f.fat_loja 
      and i.ift_empresa = f.fat_empresa 
   LEFT JOIN
      pessoa p 
      on p.pes_codpes = f.fat_codpes 
   LEFT JOIN
      pessoa p2 
      on p2.pes_codpes = f.fat_codven 
   JOIN
      loja l 
      on l.loj_codloj = f.fat_loja 
      and l.loj_codemp = f.fat_empresa 
   JOIN
      empresa e 
      on e.emp_codemp = f.fat_empresa 
   JOIN
      item ip 
      on ip.itm_item = i.ift_item and i.ift_item is not null
   LEFT JOIN
      tab_grupo g 
      on ip.itm_grupo = g.grp_codgrp
where 
   f.fat_sitfat &lt;&gt; 'C' and f.fat_tipped = 'V'</sql>
