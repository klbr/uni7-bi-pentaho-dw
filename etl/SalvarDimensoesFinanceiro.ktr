  <step>
    <name>DTempo</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>ERPDEMO</connection>
    <sql>select distinct cod_data, ano, mes, dia, dia_semana, data from
( 
  select   
    cast(to_char( cnt_datven, 'yyyyMMDD') as integer) as cod_data,
  	cast(to_char( cnt_datven, 'yyyy') as integer) as ano,
  	cast(to_char(cnt_datven, 'MM') as integer) as mes,
  	cast(to_char( cnt_datven, 'DD') as integer) as dia,
  	cast(to_char( cnt_datven, 'D') as integer) as dia_semana,
    cnt_datven  as data
  from contas  
      LEFT JOIN pessoa p  on p.pes_codpes = cnt_codpes
      left join tab_forma_pag fpg on cnt_forpag = fpg.fpg_forpag
      LEFT JOIN tab_natureza_desp on tnd_codnat = cnt_natdesp
      LEFT JOIN tab_fonte on cnt_recurs = tfo_codfon
      LEFT JOIN loja l on cnt_loja = loj_codloj 
  where cnt_tipcon in ('P', 'R')

  union ALL

  select    
     cast(to_char( cnt_datpag, 'yyyyMMDD') as integer) as cod_data,
  	cast(to_char( cnt_datpag, 'yyyy') as integer) as ano,
  	cast(to_char(cnt_datpag, 'MM') as integer) as mes,
  	cast(to_char( cnt_datpag, 'DD') as integer) as dia,
  	cast(to_char( cnt_datpag, 'D') as integer) as dia_semana,
    cnt_datpag  as data
  from contas_pag  
      LEFT JOIN pessoa p  on p.pes_codpes = cnt_codpes
      left join tab_forma_pag fpg on cnt_forpag = fpg.fpg_forpag
      LEFT JOIN tab_natureza_desp on tnd_codnat = cnt_natdesp
      LEFT JOIN tab_fonte on cnt_recurs = tfo_codfon
      LEFT JOIN loja l on cnt_loja = loj_codloj
  where cnt_tipcon in ('P', 'R') 
  ) T
  where 
     data > '30-12-1899'
  order by data






</sql>
