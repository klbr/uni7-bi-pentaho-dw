    <connection>ERPDEMO</connection>
    <sql>select  DISTINCT cnt_forpag, fpg_desfpg from
(
  select
    cnt_tipcon,
    cnt_datven as data_conta,
    case when cnt_forpag = '' or cnt_forpag is null then 'N/A' else cnt_forpag end as cnt_forpag,    
    coalesce(fpg.fpg_desfpg ,'N/A') as fpg_desfpg 
    
  from contas  
      LEFT JOIN pessoa p  on p.pes_codpes = cnt_codpes
      left join tab_forma_pag fpg on cnt_forpag = fpg.fpg_forpag
      LEFT JOIN tab_natureza_desp on tnd_codnat = cnt_natdesp
      LEFT JOIN tab_fonte on cnt_recurs = tfo_codfon
      LEFT JOIN loja l on cnt_loja = loj_codloj 

  union ALL

  select 
     cnt_tipcon,
    cnt_datven as data_conta,
    case when cnt_forpag = '' or cnt_forpag is null then 'N/A' else cnt_forpag end as cnt_forpag,        
    coalesce(fpg.fpg_desfpg ,'N/A') as fpg_desfpg   
    
  from contas_pag  
      LEFT JOIN pessoa p  on p.pes_codpes = cnt_codpes
      left join tab_forma_pag fpg on cnt_forpag = fpg.fpg_forpag
      LEFT JOIN tab_natureza_desp on tnd_codnat = cnt_natdesp
      LEFT JOIN tab_fonte on cnt_recurs = tfo_codfon
      LEFT JOIN loja l on cnt_loja = loj_codloj 
  )T
  where 
    cnt_tipcon in ('P', 'R') and data_conta > '30-12-1899'
 
</sql>
