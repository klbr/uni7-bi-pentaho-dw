  case when cnt_codemp = '' or cnt_codemp is null then 'N/A' else cnt_codemp end as cnt_codemp, 
  case when emp_razsoc = '' or emp_razsoc is null then 'N/A' else emp_razsoc end as emp_razsoc,  
   
  coalesce(Loj_denloj,'N/A') as Loj_denloj, 
  coalesce(loj_endloj,'N/A') as loj_endloj, 
  coalesce(loj_bailoj,'N/A') as loj_bailoj, 
  coalesce(loj_ufloj,'N/A') as loj_ufloj, 
  coalesce(loj_cidloj,'N/A') as loj_cidloj, 
  coalesce(loj_ceploj,'N/A') as loj_ceploj
   from
(
  select 
    'A' as st, cnt_tipcon,  cnt_codemp,   cnt_loja,
    cnt_datven as data_conta,       
    l.Loj_denloj, l.loj_endloj, l.loj_bailoj, l.loj_ufloj, l.loj_cidloj, l.loj_ceploj, emp_razsoc

  from contas  
      LEFT JOIN pessoa p  on p.pes_codpes = cnt_codpes
      left join tab_forma_pag fpg on cnt_forpag = fpg.fpg_forpag
      LEFT JOIN tab_natureza_desp on tnd_codnat = cnt_natdesp
      LEFT JOIN tab_fonte on cnt_recurs = tfo_codfon
      LEFT JOIN loja l on cnt_loja = loj_codloj 
      left JOIN empresa on emp_codemp = cnt_codemp

  union ALL

  select 
    'F' as st, cnt_tipcon,  cnt_codemp,   cnt_loja,
    cnt_datpag as data_conta,       
     l.Loj_denloj, l.loj_endloj, l.loj_bailoj, l.loj_ufloj, l.loj_cidloj, l.loj_ceploj, emp_razsoc
  from contas_pag  
      LEFT JOIN pessoa p  on p.pes_codpes = cnt_codpes
      left join tab_forma_pag fpg on cnt_forpag = fpg.fpg_forpag
      LEFT JOIN tab_natureza_desp on tnd_codnat = cnt_natdesp
      LEFT JOIN tab_fonte on cnt_recurs = tfo_codfon
      LEFT JOIN loja l on cnt_loja = loj_codloj 
      left JOIN empresa on emp_codemp = cnt_codemp
  )T
  where 
    cnt_tipcon in ('P', 'R') and data_conta > '30-12-1899'





